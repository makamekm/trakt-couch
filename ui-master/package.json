{
  "name": "ui-master",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt --port ${PORT:-\"5002\"} --hostname '0.0.0.0'",
    "docker:build": "docker build --build-arg BROWSER_BASE_URL=${BROWSER_BASE_URL:-\"https://code-reporter.com\"} --build-arg BASE_URL=${BASE_URL:-\"http://code-reporter-be\"} -t ${DOCKER_UI_IMAGE:-\"makame/code-reporter-ui\"} --network host .",
    "docker:push": "docker push ${DOCKER_UI_IMAGE:-\"makame/code-reporter-ui\"}",
    "docker:run": "docker stop ${DOCKER_UI_NAME:-\"code-reporter-ui\"} || true; docker rm ${DOCKER_UI_NAME:-\"code-reporter-ui\"} || true; docker run -d -e PORT=80 -e HOST=0.0.0.0 --restart unless-stopped --network=base --name ${DOCKER_UI_NAME:-\"code-reporter-ui\"} ${DOCKER_UI_IMAGE:-\"makame/code-reporter-ui\"}",
    "build": "nuxt build",
    "start": "nuxt start --port ${PORT:-\"5002\"} --hostname '0.0.0.0'",
    "generate": "nuxt generate",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint": "npm run lint:js",
    "test": "jest"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/composition-api": "^0.30.0",
    "@nuxtjs/moment": "^1.6.1",
    "@nuxtjs/pwa": "^3.3.5",
    "@vueuse/motion": "^2.0.0-beta.4",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.10",
    "core-js": "^3.15.1",
    "express": "^4.17.1",
    "focus-trap": "^6.6.0",
    "focus-trap-vue": "^1.1.1",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "node-fetch": "^3.1.0",
    "nuxt": "^2.15.7",
    "nuxt-ssr-screen-size": "github:hugojerez/nuxt-ssr-screen-size",
    "nuxt-use-motion": "^2.0.0-beta.5",
    "portal-vue": "^2.1.7",
    "scroll-into-view": "^1.16.0",
    "torrent-to-magnet": "^1.0.1",
    "vue-check-view": "^0.3.0",
    "vue-js-spatial-navigation": "^2.0.13",
    "vue-observe-visibility": "^1.0.0",
    "vue-resize-observer": "^2.0.16",
    "vue-snip": "^1.2.0",
